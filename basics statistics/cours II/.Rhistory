test_data <- read.csv("https://stepic.org/media/attachments/course/524/test_for_norm.csv")
test_data
aggregate(x = test_data,
by = colnames(test_data),
FUN = mean() )
aggregate(x = test_data,
by = colnames(test_data),
FUN = mean )
aggregate(x = test_data,
by = list(colnames(test_data)),
FUN = mean )
aggregate(x~y+z, test_data, mean)
aggregate(x~y+z, test_data, function(x){shapiro.test((x)$p.value)})
aggregate(x~y+z, test_data, function(x){shapiro.test(x)$p.value})
